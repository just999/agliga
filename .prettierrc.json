{
  "trailingComma":"es5",
  "semi":true,
  "tabWidth":2,
  "singleQuote":true,
  "jsxSingleQuote":true,
  "plugins":["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
